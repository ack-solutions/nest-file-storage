{
  "name": "@ackplus/nest-file-storage",
  "version": "1.1.13-beta.1",
  "description": "A flexible and feature-rich file storage solution for NestJS applications with support for local, S3, and Azure Blob Storage",
  "author": "AckPlus",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "examples",
    "LICENSE"
  ],
  "keywords": [
    "nestjs",
    "file-storage",
    "file-upload",
    "multer",
    "s3",
    "azure-blob",
    "local-storage",
    "cloud-storage",
    "file-management",
    "nestjs-storage",
    "aws-s3",
    "azure-storage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ack-solutions/nest-file-storage"
  },
  "bugs": {
    "url": "https://github.com/ack-solutions/nest-file-storage/issues"
  },
  "homepage": "https://github.com/ack-solutions/nest-file-storage#readme",
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "build": "pnpm clean && pnpm -C ../.. exec tsc -p packages/nest-file-storage/tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    },
    "@azure/storage-blob": {
      "optional": true
    }
  },
  "dependencies": {
    "concat-stream": "^2.0.0",
    "moment": "^2.30.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.713.0",
    "@aws-sdk/s3-request-presigner": "^3.713.0",
    "@azure/storage-blob": "^12.25.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@types/concat-stream": "^2.0.3",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "multer": "^1.4.5-lts.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
